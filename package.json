{
  "name": "re-react",
  "version": "0.0.1",
  "description": "React with recompose",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node_modules/.bin/webpack-dev-server --port=3000 --inline --hot",
    "start:server": "NODE_ENV=development node_modules/.bin/nodemon server",
    "storybook": "node_modules/.bin/start-storybook -p 9001 -c .storybook",
    "test": "NODE_ENV=test node_modules/.bin/jest",
    "lint:css": "node_modules/.bin/stylelint './src/**/*.styled.js'",
    "lint:js": "node_modules/.bin/eslint \"@(client|server|test)/**/*.@(js|jsx)\"",
    "lint": "npm run lint:js && npm run lint:css"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "transform": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/style-mock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js",
      "^.+\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "client"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "globals": {
      "__DEV__": true,
      "__WEB_ROOT__": "/"
    },
    "roots": [
      "<rootDir>"
    ],
    "projects": [
      "<rootDir>"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanluke/re-react.git"
  },
  "keywords": [
    "React"
  ],
  "author": "Lukasz Gonciarz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanluke/re-react/issues"
  },
  "homepage": "https://github.com/vanluke/re-react#readme",
  "dependencies": {
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.0.3",
    "jest-cli": "^22.0.3",
    "jest-enzyme": "^4.0.1",
    "jsdom": "^11.5.1",
    "json-loader": "^0.5.7",
    "nock": "^9.1.5",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.1",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "raf": "^3.4.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.6",
    "simple-webpack-clean-plugin": "^0.1.0",
    "sinon": "^4.1.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.2.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
